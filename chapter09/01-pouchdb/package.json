{
  "name": "lovely-offline",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod:shell": "ng run lovely-offline:app-shell:production",
    "build:prod": "ng build --prod --stats-json",
    "serve:prod": "ng serve --prod",
    "prod": "npm run build:prod:shell && cd dist && http-server -p 4200 -c-1",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build:prod:shell && firebase deploy",
    "analyzer": "webpack-bundle-analyzer dist/stats.json",
    "vapid": "web-push generate-vapid-keys --json",
    "pouchdb-server": "pouchdb-server --port 5984"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.0",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^7.1.0",
    "@angular/compiler": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@angular/fire": "^5.1.0",
    "@angular/forms": "^7.1.0",
    "@angular/http": "^7.1.0",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "^7.1.0",
    "@angular/platform-browser-dynamic": "^7.1.0",
    "@angular/platform-server": "^7.0.1",
    "@angular/pwa": "^0.10.6",
    "@angular/router": "^7.1.0",
    "@angular/service-worker": "7.1.0",
    "core-js": "^2.5.7",
    "firebase": "^5.5.9",
    "pouchdb-browser": "^7.0.0",
    "rxjs": "^6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.6",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/language-service": "^7.1.0",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.10",
    "@types/pouchdb": "^6.3.2",
    "@types/pouchdb-browser": "^6.1.2",
    "pouchdb-server": "^4.1.0",
    "codelyzer": "~4.5.0",
    "firebase-tools": "^6.0.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "web-push": "^3.3.3"
  }
}
